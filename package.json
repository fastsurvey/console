{
    "name": "fastsurvey-console",
    "version": "0.7.0",
    "main": "server.js",
    "author": {
        "name": "Moritz Makowski",
        "email": "moritz@dostuffthatmatters.dev",
        "url": "https://dostuffthatmatters.dev/"
    },
    "license": "UNLICENSED",
    "scripts": {
        "develop": "export VITE_ENV=\"development\" && vite --host",
        "check-ts": "tsc --noEmit",
        "build": "export VITE_COMMIT_SHA=\"$(git rev-parse --short --verify HEAD)\" && tsc && vite build",
        "build:development": "export VITE_ENV=\"development\" && yarn build",
        "build:production": "export VITE_ENV=\"production\" && yarn build",
        "serve": "vite preview",
        "analyze": "source-map-explorer 'dist/assets/*.js'",
        "cypress:integration": "cypress open",
        "cypress:component": "yarn tailwind:build && cypress open-ct",
        "tailwind:build": "npx tailwindcss -i src/styles/tailwind.css -o src/styles/tailwind.out.css"
    },
    "dependencies": {
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@cypress/react": "^5.10.3",
        "@cypress/vite-dev-server": "^2.2.0",
        "@cypress/webpack-dev-server": "^1.7.0",
        "@headlessui/react": "^1.4.1",
        "@heroicons/react": "^1.0.4",
        "@sentry/react": "^6.15.0",
        "@tailwindcss/line-clamp": "^0.3.0",
        "@tailwindcss/typography": "^0.5.0",
        "@types/axios": "^0.14.0",
        "@types/jest": "^24.0.0",
        "@types/js-cookie": "^2.2.7",
        "@types/lodash": "^4.14.171",
        "@types/node": "^12.0.0",
        "@types/react": "^16.9.51",
        "@types/react-dom": "^16.9.8",
        "@types/react-redux": "^7.1.17",
        "@types/react-router-dom": "^5.1.6",
        "@types/react-scroll": "^1.8.2",
        "@vitejs/plugin-react-refresh": "^1.3.5",
        "autoprefixer": "^10.4.0",
        "axios": "^0.21.1",
        "cypress": "^8.7.0",
        "jest": "^27.0.6",
        "js-cookie": "^2.2.1",
        "lodash": "^4.17.21",
        "postcss": "^8.4.5",
        "react-markdown": "^7.1.1",
        "remark-gfm": "^3.0.1",
        "remark-math": "^5.1.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.4",
        "react-router-dom": "^5.2.0",
        "react-scroll": "^1.8.1",
        "source-map-explorer": "^2.5.2",
        "tailwindcss": "^3.0.7",
        "tailwindcss-interaction-variants": "^5.0.0",
        "typeface-quicksand": "^1.1.13",
        "typescript": "^4.3.5",
        "vite": "^2.4.1"
    }
}
